    "virtualMachineName1": {
      "value": "vmCentOsFresh"
    },
    "virtualMachineComputerName1": {
      "value": "vmCentOsFresh"
    },
    "osDiskType": {
      "value": "Premium_LRS"
    },
    "osDiskDeleteOption": {
      "value": "Delete"
    },
    "virtualMachineSize": {
      "value": "Standard_F4s_v2"
    },
    "nicDeleteOption": {
      "value": "Detach"
    },
    "hibernationEnabled": {
      "value": false
    },
    "adminUsername": {
      "value": "PedroSilva"
    },
    "adminPassword": {
      "value": null
    },
    "virtualMachine1Zone": {
      "value": "1"
    }
